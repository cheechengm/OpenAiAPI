{
  "rewrites": [
    { "source": "/(.*)", "destination": "/app.js" }
  ],
  "functions": {
    "/openai/meta": {
      "handler": "/controllers/openaiController",
      "runtime": "nodejs18.x"
    },
    "/openai/image": {
      "handler": "/controllers/openaiController",
      "runtime": "nodejs18.x"
    }
  }
}
