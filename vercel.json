{
  "rewrites": [
    { "source": "/openai/meta", "destination": "/openai/meta" },
    { "source": "/openai/image", "destination": "/openai/image" },
    { "source": "/(.*)", "destination": "/" }
  ],
  "routes": [
    { "src": "/openai/meta", "methods": ["POST"], "dest": "/openai/meta" },
    { "src": "/openai/image", "methods": ["POST"], "dest": "/openai/image" }
  ]
}
