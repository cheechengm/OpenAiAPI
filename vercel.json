{
  "version": 2,
  "builds": [
    {
      "src": "./app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/openai/meta",
      "dest": "/openai/meta"
    },
    {
      "src": "/openai/image",
      "dest": "/openai/image"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
